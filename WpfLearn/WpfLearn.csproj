        <Nullable>enable</Nullable>
        <UseWPF>true</UseWPF>
    </PropertyGroup>

    <ItemGroup>
      <Folder Include="image" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="image\bsu.jpg" />
